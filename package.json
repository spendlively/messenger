{
  "name": "Opios",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "pack": "build --dir",
    "dist": "build",
    "start": "./node_modules/.bin/electron .",
    "build": "electron-packager . Opios --all --version=1.3.4 --app-version=0.0.1 --out=dist --ignore=dist --prune --overwrite --icon=opios.icns",
    "build:linux": "electron-packager . Opios --save-dev --platform=linux --arch=x64 --version=1.3.4 --app-version=0.0.1 --out=dist --ignore=dist --prune --overwrite --icon=opios.icns",
    "build:darwin": "electron-packager . Opios --platform=darwin --arch=x64 --version=1.3.4 --app-version=0.0.1 --build-version=0.0.1 --out=dist --ignore=dist --prune --overwrite --icon=opios.icns",
    "build:mas": "electron-packager . Opios --platform=mas --arch=x64 --version=1.3.4 --app-version=0.0.1 --build-version=0.0.1 --out=dist --ignore=dist --prune --overwrite --icon=opios.icns",
    "build:win32": "electron-packager . Opios --platform=win32 --arch=ia32 --version=1.3.4 --app-version=0.0.1 --out=dist --ignore=dist --prune --overwrite --icon=opios.icns",
    "build:win64": "electron-packager . Opios --platform=win32 --arch=x64 --version=1.3.4 --app-version=0.0.1 --out=dist --ignore=dist --prune --overwrite"
  },
  "build": {
    "dmg": {},
    "win": {},
    "linux": {}
  },
  "dependencies": {
    "auto-launch": "^5.0.1",
    "node-notifier": "^4.6.1",
    "sanitizer": "^0.1.3"
  },
  "description": "bla bla bla",
  "author": "spendlively",
  "repository": {},
  "license": "",
  "devDependencies": {
    "electron-builder": "^7.11.4",
    "electron-prebuilt": "^1.3.4",
    "auto-launch": "^5.0.1",
    "electron-installer-dmg": "^0.1.2",
    "electron-packager": "^8.0.0"
  }
}
